<COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
    COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
    IS_MULTIPLYING_OUTPUTS="false"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true">
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" READONLY="true" REQUIRED="true" NUM_ROW="5">
      <TABLE READONLY="true" IF="OUTPUT_DOCUMENT=='false'">
        <COLUMN NAME="Header" TYPE="id_String" READONLY="true" />
        <COLUMN NAME="Body" TYPE="id_String" READONLY="true" />
        <COLUMN NAME="Fault" TYPE="id_String" READONLY="true" />
      </TABLE>
      <TABLE READONLY="true" IF="OUTPUT_DOCUMENT=='true'">
        <COLUMN NAME="Soap" TYPE="id_Document" READONLY="true" />
      </TABLE>
    </PARAMETER>
    <PARAMETER NAME="USE_NTLM" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="10">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="NTLM_DOMAIN" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="10" SHOW_IF="USE_NTLM == 'true'">
      <DEFAULT>"192.168.0.100"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="NEED_AUTH" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="20">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SOAP_DIGEST_AUTH" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="30" SHOW_IF="NEED_AUTH == 'true'">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="AUTH_USERNAME" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="40" SHOW_IF="NEED_AUTH == 'true'">
      <DEFAULT>"username"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="AUTH_PASSWORD" FIELD="PASSWORD" REQUIRED="true"
      NUM_ROW="40" SHOW_IF="NEED_AUTH == 'true'">
      <DEFAULT>"password"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="USE_PROXY" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="50">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PROXY_HOST" FIELD="TEXT"
      NUM_ROW="51" SHOW_IF="USE_PROXY == 'true'">
      <DEFAULT>"61.163.92.4"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PROXY_PORT" FIELD="TEXT"
      NUM_ROW="52" SHOW_IF="USE_PROXY == 'true'">
      <DEFAULT>8080</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PROXY_USERNAME" FIELD="TEXT"
      NUM_ROW="53" SHOW_IF="USE_PROXY == 'true'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PROXY_PASSWORD" FIELD="PASSWORD"
      NUM_ROW="54" SHOW_IF="USE_PROXY == 'true'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="NEED_SSL_TO_TRUSTSERVER" FIELD="CHECK" REQUIRED="true" NUM_ROW="60">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SSL_TRUSTSERVER_TRUSTSTORE" FIELD="TEXT" NUM_ROW="65" SHOW_IF="NEED_SSL_TO_TRUSTSERVER == 'true'">
      <DEFAULT>"C:/clientTrustStore.trustStore"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SSL_TRUSTSERVER_PASSWORD" FIELD="PASSWORD" NUM_ROW="65" SHOW_IF="NEED_SSL_TO_TRUSTSERVER == 'true'">
      <DEFAULT>"password"</DEFAULT>
    </PARAMETER>


    <PARAMETER NAME="ENDPOINT" FIELD="TEXT" REQUIRED="true" NUM_ROW="70">
      <DEFAULT>"http://www.webservicex.net/globalweather.asmx"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="ACTION" FIELD="TEXT" REQUIRED="true" NUM_ROW="80">
      <DEFAULT>"http://www.webserviceX.NET/GetWeather"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SOAP_VERSION" FIELD="CLOSED_LIST" NUM_ROW="90">
      <ITEMS DEFAULT="SOAP_1.1">
        <ITEM NAME="SOAP_1.1" VALUE="Soap11" />
        <ITEM NAME="SOAP_1.2" VALUE="Soap12" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER NAME="OUTPUT_DOCUMENT" FIELD="CHECK" REQUIRED="true" NUM_ROW="95">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="USE_MESSAGE_FROM_SCHEMA" FIELD="CHECK" REQUIRED="true" NUM_ROW="100">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SOAPMESSAGE_FROM_SCHEMA" FIELD="PREV_COLUMN_LIST" NUM_ROW="110" SHOW_IF="USE_MESSAGE_FROM_SCHEMA=='true'" />
    <PARAMETER NAME="SOAPMESSAGE" FIELD="MEMO" NUM_ROW="110" NB_LINES="10" SHOW_IF="USE_MESSAGE_FROM_SCHEMA=='false'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>
    <PARAMETER
        NAME="USE_KERBEROS"
        FIELD="CHECK"
        NUM_ROW="10"
      >
        <DEFAULT>false</DEFAULT>
      </PARAMETER>

      <PARAMETER
        NAME="KERBEROS_CONFIGURATION"
        FIELD="COMPONENT_LIST"
        REQUIRED="true" FILTER="tSetKerberosConfiguration"
        NUM_ROW="10"
        SHOW_IF="USE_KERBEROS == 'true'"
      />
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="KoddexTalendHelpers" MODULE="KoddexTalendHelpers.jar" REQUIRED="true"/>
      <IMPORT NAME="activation" MODULE="activation.jar" MVN="mvn:org.talend.libraries/activation/6.0.0" REQUIRED="true" BundleID="" />
      <IMPORT NAME="jaxqname" MODULE="jax-qname.jar" MVN="mvn:org.talend.libraries/jax-qname/6.0.0" REQUIRED="true" BundleID="" />
      <IMPORT NAME="jaxpapi" MODULE="jaxp-api.jar" MVN="mvn:org.talend.libraries/jaxp-api/6.0.0" REQUIRED="true" BundleID="" />
      <IMPORT NAME="jaxpri" MODULE="jaxp-ri.jar" MVN="mvn:org.talend.libraries/jaxp-ri/6.0.0" REQUIRED="true" BundleID="" />
      <IMPORT NAME="saajapi" MODULE="saaj-api-1.3.jar" MVN="mvn:org.talend.libraries/saaj-api-1.3/6.0.0" REQUIRED="true" BundleID="" />
      <IMPORT NAME="saajimpl" MODULE="saaj-impl-1.3.2.jar" MVN="mvn:org.talend.libraries/saaj-impl-1.3.2/6.0.0" REQUIRED="true" BundleID="" />
      <IMPORT NAME="jdom" MODULE="jdom-1.1.jar" MVN="mvn:org.talend.libraries/jdom-1.1/6.0.0" UrlPath="platform:/plugin/org.talend.libraries.jdom/lib/jdom-1.1.jar" REQUIRED="true" BundleID="org.apache.servicemix.bundles.jdom" />
      <IMPORT NAME="talendsoap" MODULE="talend-soap-2.0-20160719.jar" MVN="mvn:org.talend.libraries/talend-soap-2.0-20160719/6.0.0" REQUIRED="true" />
      <IMPORT NAME="Java_xercesImpl" MODULE="xercesImpl.jar" MVN="mvn:org.talend.libraries/xercesImpl/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.xml/lib/xercesImpl.jar" REQUIRED="true" BundleID="" />
      <IMPORT NAME="commons-codec" MODULE="commons-codec-1.9.jar" MVN="mvn:org.talend.libraries/commons-codec-1.9/6.1.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-codec-1.9.jar" REQUIRED="true" BundleID="" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS/>

</COMPONENT>